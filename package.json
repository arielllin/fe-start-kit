{
  "name": "mxfe-start-kit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "cross-env ENV_CONFIG=production vue-cli-service lint",
    "dev": "cross-env ENV_CONFIG=develop vue-cli-service serve",
    "build:prod": "cross-env ENV_CONFIG=production vue-cli-service build",
    "inspect:dev": "cross-env ENV_CONFIG=develop vue-cli-service inspect > ./build/inspect.webpack.dev.js",
    "inspect:prod": "cross-env ENV_CONFIG=production vue-cli-service inspect --mode production > ./build/inspect.webpack.prod.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^2.6.5",
    "js-cookie": "^2.2.1",
    "moment": "^2.24.0",
    "vue": "^2.6.10",
    "vue-i18n": "^8.14.1",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^6.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "plop": "^2.4.0",
    "recast": "^0.18.3",
    "svg-sprite-loader": "^4.1.6",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
